{
  "productName": "LeadMiner Ultimate",
  "version": "0.1.0",
  "identifier": "com.leadminer.ultimate",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "LeadMiner Ultimate",
        "width": 1200,
        "height": 800,
        "resizable": true
      }
    ],
    "security": {
      "csp": "default-src 'self'; script-src 'self' 'unsafe-inline'; style-src 'self' 'unsafe-inline'; img-src 'self' data: https:; connect-src 'self' http://localhost:* https:*;"
    }
  },
  "bundle": {
    "active": true,
    "icon": ["icons/icon.ico"]
  }
}